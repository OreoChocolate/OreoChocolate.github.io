
<ul class="card-text font-weight-light list-group">
  {% assign volunteer = data[1] | sort: 'startDate' | reverse %}
  {% for content in volunteer %}
    <li class="list-group-item" style="border: none !important;">
          <h6 class="" style="font-size: 1.0rem">{{ content.organization }}</h6>
          <ul class="items" style="list-style-type: circle;">
            {% for item in content.highlights %}
              <li>
                <span class="item">{{ item }}</span>
              </li>
            {% endfor %}
          </ul>
    </li>
  {% endfor %}
</ul>